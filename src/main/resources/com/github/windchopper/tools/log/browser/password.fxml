<?xml version="1.0" encoding="UTF-8"?>

<?import com.github.windchopper.tools.log.browser.actions.*?>
<?import com.github.windchopper.tools.log.browser.configuration.*?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?language javascript?>

<!--suppress JavaFxUnresolvedFxIdReference-->
<!--suppress JSUnusedLocalSymbols-->

<GridPane xmlns="http://javafx.com/javafx/12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.windchopper.tools.log.browser.PasswordStageController">
    <Label GridPane.columnIndex="1" GridPane.rowIndex="0" GridPane.fillWidth="true" GridPane.hgrow="ALWAYS" text="%com.github.windchopper.tools.log.browser.password.password" wrapText="true">
        <GridPane.margin>
            <Insets top="10.0" left="10.0" bottom="10.0" right="10.0"/>
        </GridPane.margin>
    </Label>
    <TextField fx:id="commandTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.fillWidth="true"
               GridPane.hgrow="ALWAYS">
        <GridPane.margin>
            <Insets top="10.0" left="10.0" bottom="10.0" right="10.0"/>
        </GridPane.margin>
    </TextField>
    <HBox GridPane.columnIndex="0" GridPane.rowIndex="3" GridPane.columnSpan="2" GridPane.fillWidth="true"
          GridPane.hgrow="ALWAYS" alignment="CENTER_RIGHT">
        <GridPane.margin>
            <Insets top="10.0" left="10.0" bottom="10.0" right="10.0"/>
        </GridPane.margin>
        <Button fx:id="okButton" text="%com.github.windchopper.tools.log.browser.password.ok" defaultButton="true" minWidth="100.0">
            <HBox.margin>
                <Insets left="4.0"/>
            </HBox.margin>
        </Button>
        <Button fx:id="browseButton" text="%com.github.windchopper.tools.log.browser.password.cancel" minWidth="100.0">
            <HBox.margin>
                <Insets left="4.0"/>
            </HBox.margin>
        </Button>
    </HBox>
</GridPane>
